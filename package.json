{
	"name": "pokepi",
	"version": "1.0.0",
	"description": "Pokepi est une API permettant à l'utilisateur de choisir les pokémons dans son propre pokédex",
	"main": "index.ts",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"migration": "npx sequelize-cli db:migrate",
		"remove_migration": "npx sequelize-cli db:migrate:undo:all",
		"seed": "npx sequelize-cli db:seed:all",
		"dev": "nodemon src/index.ts",
		"prettier": "prettier --write ."
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SkynixFr/PokePI.git"
	},
	"keywords": [
		"Pokepi",
		"Pokémon",
		"Pokédex",
		"API"
	],
	"author": "SkynixFr, Luffysonic",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/SkynixFr/PokePI/issues"
	},
	"homepage": "https://github.com/SkynixFr/PokePI#readme",
	"dependencies": {
		"axios": "^1.2.2",
		"bcrypt": "^5.1.0",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"express-rate-limit": "^6.7.0",
		"helmet": "^6.0.1",
		"jsonwebtoken": "^9.0.0",
		"memory-cache": "^0.2.0",
		"mysql2": "^2.3.3",
		"nodemon": "^2.0.20",
